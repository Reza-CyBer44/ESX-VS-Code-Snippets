    "Get ESX Object": {
        "prefix": [
            "GetESX",
            "ESX = nil",
            "GetSharedObject"
        ],
        "body": [
            "ESX = nil \nCitizen.CreateThread(function() \nwhile ESX == nil do \nTriggerEvent('esx:getSharedObject', function(obj) ESX = obj end) \nCitizen.Wait(0) \nend \nend)"
        ],
        "description": "Get ESX Object"
    },
